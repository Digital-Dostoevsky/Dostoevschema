                  <zeroOrMore>
                     <choice>
                        <ref name="tei_model.noteLike"/>
                        <ref name="tei_model.biblLike"/>
                     </choice>
                  </zeroOrMore>
               </group>
            </optional>
            <zeroOrMore>
               <ref name="tei_population"/>
            </zeroOrMore>
         </group>
